                    <button class="btn btn-sm btn-outline-secondary" data-svg-target="front_upper_arm_left">Oberarm li.</button>
                    <button class="btn btn-sm btn-outline-secondary" data-svg-target="front_lower_arm_right">Unterarm re.</button>
                    <button class="btn btn-sm btn-outline-secondary" data-svg-target="front_lower_arm_left">Unterarm li.</button>
                    <button class="btn btn-sm btn-outline-secondary" data-svg-target="front_hand_right">Hand re.</button>
                    <button class="btn btn-sm btn-outline-secondary" data-svg-target="front_hand_left">Hand li.</button>
                    <button class="btn btn-sm btn-outline-secondary" data-svg-target="front_genital">Genitalregion</button>
                    <button class="btn btn-sm btn-outline-secondary" data-svg-target="front_upper_leg_right">Oberschenkel re.</button>
                    <button class="btn btn-sm btn-outline-secondary" data-svg-target="front_upper_leg_left">Oberschenkel li.</button>
                    <button class="btn btn-sm btn-outline-secondary" data-svg-target="front_lower_leg_right">Unterschenkel re.</button>
